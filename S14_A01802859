# Ejercico 1
edad = int(input("ingresa tu edad"))

if edad >= 18:
    print ("eres mayor de edad") 

# Ejercico 2    
edad = int(input("ingresa tu edad"))

if edad >= 13 and edad <= 19:
    print ("eres adolescente")

#Ejercico 3
años = int(input("ingresa tu año de nacimiento"))
años = 2025 - años
print ("tienes", años, "años")
